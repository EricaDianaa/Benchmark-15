@using Benchmark_15.Models
@{
    ViewBag.Title = "SelectClienti";
}

<h2>SelectClienti</h2>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Numero Ciente</th>
            <th scope="col">Nome</th>
            <th scope="col">Cognome</th>
            <th scope="col">Città</th>
            <th scope="col">Provincia</th>
            <th scope="col">Codice fiscale</th>
            <th scope="col">Telefono</th>
            <th scope="col">Cellulare</th>
            <th scope="col">Email</th>
            <th scope="col"></th>
        </tr>
    </thead>
    @foreach (Clienti c in Model)
    {
        <tbody>
            <tr>
                <td>@c.IdCliente</td>
                <td>@c.Nome</td>
                <td>@c.Cognome</td>
                <td>@c.Città</td>
                <td>@c.Provincia</td>
                <td>@c.CodiceFiscale</td>
                <td>@c.Telefono</td>
                <td>@c.Cellulare</td>
                <td>@c.Email</td>
                <td><a href="EditCliente?Id=@c.IdCliente">Modifica</a></td>
            </tr>

        </tbody>
    }
</table>
<a href="CreateCliente">Aggiungi Cliente</a>