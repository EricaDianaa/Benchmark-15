@using Benchmark_15.Models
@{
    ViewBag.Title = "SelectServizi";
}

<h2>SelectPrenotazione</h2>


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    @foreach (Servizi c in Model)
    {
        <tbody>
            <tr>
                <td>@c.IdServizio</td>
                <td>@c.TipoServizi</td>
                <td>@c.Quantità</td>
                <td>@c.Prezzo</td>
                <td>@c.Data</td>
                <td>@c.IdPrenotazioni</td>
                <td><a href="EditServizio?Id=@c.IdServizio">Modifica</a></td>
            </tr>

        </tbody>
    }
</table>