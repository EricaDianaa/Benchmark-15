@using Benchmark_15.Models
@{
    ViewBag.Title = "SelectServizi";
}

<h2>SelectPrenotazione</h2>


<table class="table">
    <thead>
        <tr>
            <th scope="col">Numero Servizio</th>
            <th scope="col">Tipo sevrizio</th>
            <th scope="col">Quantità</th>
            <th scope="col">Prezzo</th>
            <th scope="col">Data</th>
            <th scope="col">Numero Prenotazione</th>
            <th scope="col"></th>



        </tr>
    </thead>
    @foreach (Servizi c in Model)
    {
        <tbody>
            <tr>
                <td>@c.IdServizio</td>
                <td>@c.TipoServizi</td>
                <td>@c.Quantità</td>
                <td>@c.Prezzo</td>
                <td>@c.Data</td>
                <td>@c.IdPrenotazioni</td>
                <td><a href="EditServizio?Id=@c.IdServizio">Modifica</a></td>
            </tr>

        </tbody>
    }
</table>